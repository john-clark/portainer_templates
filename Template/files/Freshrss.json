{
  "type": 1,
  "title": "Freshrss",
  "name": "Freshrss",
  "description": "A free, self-hostable rss aggregator",
  "platform": "linux",
  "logo": "https://raw.githubusercontent.com/frdmir/portainer_templates/master/images/freshrss-icon.png",
  "image": "linuxserver/freshrss:latest",
  "ports": [
    "80/tcp"
  ],
  "volumes": [
    {
      "container": "/config"
    }
  ],
  "env": [
    {
      "name": "PUID",
      "description": "UserID",
      "label": "PUID"
    },
    {
      "name": "PGID",
      "description": "GroupID",
      "label": "PGID"
    },
    {
      "name": "TZ",
      "description": "Specify a timezone to use e.g. Europe/London",
      "label": "TZ"
    }
  ],
  "categories": [
    "Other"
  ]
}
